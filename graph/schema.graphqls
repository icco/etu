# Built in scalars: https://gqlgen.com/reference/scalars/
scalar Time
scalar Map
scalar Upload


type User {
  id: ID!
  name: String!
  email: String!
}

type Entry {
  id: ID!
  datetime: Time!
  text: String!
  user: User!
}

type Query {
  entries: [Entry!]!
}

input NewEntry {
  text: String!
  datetime: Time!
  userId: String!
}

type Mutation {
  createEntry(input: NewEntry!): Entry!
}
